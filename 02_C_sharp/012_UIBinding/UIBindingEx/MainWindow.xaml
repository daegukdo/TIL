<Window x:Class="UIBindingEx.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:uiComponents="clr-namespace:UIBindingEx.UIComponents"
        Title="MainWindow" Height="860" Width="1480" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0">
            <uiComponents:MenuBar x:Name="ManuBar"></uiComponents:MenuBar>
            <uiComponents:ToolBar x:Name="ToolBar"></uiComponents:ToolBar>
        </StackPanel>
        <Grid Grid.Row="1">
            
        </Grid>
    </Grid>
</Window>
